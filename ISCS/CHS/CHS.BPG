PROJECTS = ISCS.exe ISCS.chs
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

ISCS.exe: ..\ISCS.DPR
  $(DCC)

ISCS.chs: ISCS.dpr
  $(DCC)


