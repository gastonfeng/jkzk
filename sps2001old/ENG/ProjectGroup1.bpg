#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = sps2001.exe sps2001.eng
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

sps2001.exe: ..\sps2001.DPR
  $(DCC)

sps2001.eng: sps2001.dpr
  $(DCC)


